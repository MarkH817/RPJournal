{
  "name": "rpjournal",
  "version": "0.0.0",
  "description": "A journaling tool for holding RPG session notes",
  "scripts": {
    "format": "npm run prettier && npm run lint -- --fix",
    "lint": "eslint --ignore-path .gitignore .",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|yml|yaml|css|less|ts|md|graphql|mdx)\"",
    "start": "node src",
    "test": "npm run lint && npm run type && jest --coverage",
    "type": "tsc --pretty"
  },
  "license": "MIT",
  "main": "src/index.js",
  "author": "Mark Hernandez <mark@lion-byte.com>",
  "devDependencies": {
    "@types/jest": "^23.3.11",
    "@types/node": "^10.12.18",
    "eslint": "^5.11.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-jsx": "^6.0.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.2",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "typescript": "^3.2.2"
  },
  "dependencies": {}
}
