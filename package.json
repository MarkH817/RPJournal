    "@types/graphql": "^14.0.5",
    "@types/jest": "^23.3.13",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/next": "^7.0.7",
    "@types/node": "^10.12.20",
    "@types/nprogress": "0.0.29",
    "@types/react": "^16.8.0",
    "@types/react-test-renderer": "^16.0.3",
    "@types/react-timeago": "^4.1.0",
    "@types/sanitize-html": "^1.18.2",
    "@types/styled-components": "^4.1.7",
